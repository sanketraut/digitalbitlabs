{
    "name": "digitalbit/forms",
    "type": "library",
    "description": "Form Builder package for Laravel 5.5",
    "keywords": [
        "Digitalbit",
        "Forms",
        "Formbuilder"
    ],
    "homepage": "https://github.com/sanketraut/formbuilder",
    "license": "MIT",
    "authors": [{
        "name": "Sanket Raut",
        "email": "sanket@digitalbit.in",
        "homepage": "https://digitalbit.in",
        "role": "Developer"
    }],
    "require": {
        "illuminate/support": "~5.1",
        "php": "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Digitalbit\\Forms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Digitalbit\\Forms\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit tests/FormBuilderTest.php",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "config": {
        "sort-packages": true
    }
}